            "supplyChainAPI",
            {
              "Fn::Join": [
                "",
                [
                  "supplyChainAPI",
                  "-",
                  {
                    "Ref": "env"
                  }
                ]
              ]
            }
          ]
        },
        "Environment": {
          "Variables": {
            "ENV": {
              "Ref": "env"
            },
            "REGION": {
              "Ref": "AWS::Region"
            },
            "PEER_ENDPOINT": {
              "Ref": "peerEndpoint"
            },
            "ORDERER_ENDPOINT": {
              "Ref": "ordererEndpoint"
            },
            "CA_ENDPOINT": {
              "Ref": "caEndpoint"
            },
            "CHANNEL_NAME": {
              "Ref": "channelName"
            },
            "CHAINCODE_NAME": {
              "Ref": "chaincodeName"
            },
            "MSP_ID": {
              "Ref": "mspId"
            },
            "MEMBER_ID": {
              "Ref": "memberId"
            },
            "NETWORK_ID": {
              "Ref": "networkId"
            },
            "USER_CERT": {
              "Ref": "userCert"
            },
            "USER_PRIVATE_KEY": {
              "Ref": "userPrivateKey"
            },
            "TLS_CERT": {
              "Ref": "tlsCert"
            },
            "CA_TLS_CERT": {
              "Ref": "caTlsCert"
            }
          }
        },
        "Role": {
          "Fn::GetAtt": [
            "LambdaExecutionRole",
            "Arn"
          ]
        },
        "Runtime": "nodejs22.x",
        "Layers": [],
        "Timeout": 60,
        "VpcConfig": {
          "SecurityGroupIds": [
            "sg-035d562b317e7ccb2"
          ],
          "SubnetIds": [
            "subnet-0d38c80717a1ade86",
            "subnet-04424a3e27545e05f"
          ]
        }
      }
    },
    "LambdaExecutionRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "RoleName": {
          "Fn::If": [
            "ShouldNotCreateEnvResources",
            "supplychainambLambdaRoled72725b1",
            {
              "Fn::Join": [
                "",
                [
                  "supplychainambLambdaRoled72725b1",
                  "-",
                  {
                    "Ref": "env"
                  }
                ]
              ]
            }
          ]
        },
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Principal": {
                "Service": [
                  "lambda.amazonaws.com"
                ]
              },
              "Action": [
                "sts:AssumeRole"
              ]
            }
          ]
        }
      }
    },
    "lambdaexecutionpolicy": {
      "DependsOn": [
        "LambdaExecutionRole"
      ],
      "Type": "AWS::IAM::Policy",
