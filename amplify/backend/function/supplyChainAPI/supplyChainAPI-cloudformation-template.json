    "caEndpoint": {
      "Type": "String",
      "Default": "ca.m-ktgjmti7hfgtzku7ecmps4fquu.n-cfcacd47iza7dalldsyz32fuzy.managedblockchain.us-east-1.amazonaws.com:30002"
